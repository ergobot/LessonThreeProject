<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.lessonthreeproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuestionOneActivity"></activity>
        <activity android:name=".QuestionTwoActivity"></activity>
        <activity android:name=".QuestionThreeActivity"></activity>
        <activity android:name=".QuestionFourActivity"></activity>
        <activity android:name=".QuestionFiveActivity"></activity>
        <activity android:name=".CompleteActivity"></activity>

    </application>

</manifest>